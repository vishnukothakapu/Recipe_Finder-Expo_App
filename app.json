{
  "expo": {
    "name": "Recipe Finder",
    "slug": "recipe-finder-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.vishnukothakapu27.recipefinderexpo"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ae51f490-5cc5-4a8d-b57a-9dfe9c7a653a"
      }
    }
  }
}
